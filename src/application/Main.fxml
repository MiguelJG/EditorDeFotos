<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SplitMenuButton?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="170.0" prefWidth="529.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ImageControl">
   <children>
      <Button layoutY="2.0" mnemonicParsing="false" onAction="#chargeImage" text="Nueva Imagen" />
      <SplitMenuButton layoutX="102.0" layoutY="2.0" mnemonicParsing="false" onAction="#histograma" text="Histograma">
        <items>
            <MenuItem mnemonicParsing="false" onAction="#histogramaAcumulado" text="Histograma Acumulado" />
          <MenuItem mnemonicParsing="false" onAction="#duplicar" text="Duplicar" />
          <MenuItem mnemonicParsing="false" onAction="#blancoNegro" text="Blanco/Negro" />
            <MenuItem mnemonicParsing="false" onAction="#crearPorPuntos" text="PorTramos" />
            <MenuItem mnemonicParsing="false" onAction="#ecualizar" text="Ecualizar" />
        </items>
      </SplitMenuButton>
   </children>
</AnchorPane>
